{
  "name": "@bx-fe/e2e-runner",
  "version": "1.0.0",
  "description": "E2E runner monorepo",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "prepare": "husky install",
    "ci": "yarn install --frozen-lockfile",
    "ci:ignore-scripts": "yarn ci --ignore-scripts",
    "lerna:bootstrap": "lerna bootstrap",
    "lerna:clean": "lerna clean --yes",
    "lerna:publish": "lerna publish from-package --yes",
    "build": "node scripts/build.js",
    "build:dev": "NODE_ENV=development yarn build",
    "build:cli:packages": "yarn build cli-packages",
    "build:cli:configs": "yarn build cli-configs",
    "clean": "rimraf packages/*/dist",
    "test": "jest",
    "test:changed": "yarn test --changedSince origin/master",
    "test:coverage": "yarn test --coverage",
    "lint": "eslint --ext .js,.ts,.svelte ./ --quiet",
    "lint:fix": "yarn lint --fix",
    "types": "node scripts/check-types/base.js",
    "types:changed": "node scripts/check-types/changed.js",
    "version:check": "node scripts/check-version.js",
    "version:up": "node scripts/up-version.js"
  },
  "author": "rsharipov",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "publishConfig": {
    "registry": "https://nexus.s.o3.ru/repository/npm-private/"
  },
  "_moduleAliases": {
    "~": "."
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/plugin-transform-runtime": "7.14.5",
    "@babel/preset-env": "7.14.8",
    "@babel/preset-typescript": "7.14.5",
    "@bx-fe/eslint-plugin-svelte": "1.1.5",
    "@bx-fe/eslint-plugin-ts": "1.2.2",
    "@lerna/project": "4.0.0",
    "@rollup/plugin-commonjs": "20.0.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.0.4",
    "@types/jest": "26.0.24",
    "@types/tmp": "0.2.3",
    "@zerollup/ts-transform-paths": "1.7.18",
    "axios": "0.21.1",
    "commander": "8.1.0",
    "compare-versions": "4.1.3",
    "deepmerge": "4.2.2",
    "fs-extra": "10.0.1",
    "husky": "7.0.2",
    "inquirer": "8.2.0",
    "jest": "27.0.6",
    "jest-junit": "13.0.0",
    "lerna": "4.0.0",
    "lint-staged": "11.1.2",
    "module-alias": "2.2.2",
    "nock": "13.1.2",
    "ora": "5.4.1",
    "rimraf": "3.0.2",
    "rollup": "2.53.3",
    "rollup-plugin-delete": "2.0.0",
    "rollup-plugin-dts": "3.0.2",
    "rollup-plugin-filesize": "9.1.2",
    "rollup-plugin-node-externals": "4.0.0",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.30.0",
    "simple-git": "3.2.6",
    "tmp": "0.2.1",
    "ts-jest": "27.0.4",
    "ts-node": "10.1.0",
    "ttypescript": "1.5.12",
    "typescript": "4.1.5"
  },
  "resolutions": {
    "@ampproject/remapping": "2.1.2",
    "@babel/generator": "7.14.5",
    "@babel/parser": "7.14.6",
    "@jridgewell/trace-mapping": "0.3.4",
    "caniuse-lite": "1.0.30001325"
  }
}
